{
    "name": "dodging.bullets.testing.with.react",
    "version": "1.0.0",
    "private": true,
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}",
            "!src/registerServiceWorker.js",
            "!src/setupTests.js"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 70,
                "functions": 70,
                "lines": 70,
                "statements": 70
            }
        }
    },
    "dependencies": {
        "bootstrap": "^3.3.7",
        "enzyme": "^3.3.0",
        "jsonwebtoken": "^8.2.0",
        "material-ui": "^1.0.0-beta.28",
        "material-ui-icons": "^1.0.0-beta.17",
        "moment": "^2.22.1",
        "numeral": "^2.0.6",
        "react": "^16.2.0",
        "react-async-button": "^0.2.0",
        "react-bootstrap": "^0.31.5",
        "react-dom": "^16.2.0",
        "react-router-dom": "^4.2.2",
        "react-scripts": "1.1.0",
        "react-tap-event-plugin": "^3.0.2"
    },
    "scripts": {
        "launch": "npm start",
        "build": "react-scripts build",
        "start": "react-scripts start",
        "test": "react-scripts test --env=jsdom",
        "test-coverage": "react-scripts test --env=jsdom --collectCoverage=true ",
        "eject": "react-scripts eject"
    },
    "devDependencies": {
        "enzyme-adapter-react-16": "^1.1.1",
        "eslint": "^4.16.0",
        "eslint-plugin-react": "^7.5.1",
        "eslint-watch": "^3.1.3",
        "jest": "^21.2.1",
        "jest-cli": "^21.2.1",
        "jest-junit": "^3.5.0",
        "react-app-env": "^1.2.0",
        "regenerator-runtime": "^0.11.1",
        "sinon": "^4.1.6",
        "sinon-stub-promise": "^4.0.0"
    }
}